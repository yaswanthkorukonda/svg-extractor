{
  "manifest_version": 3,
  "name": "SVG Extractor",
  "version": "1.0",
  "description": "Extract and download SVGs from any web page.",
  "permissions": ["activeTab", "scripting", "downloads"],
  "host_permissions": [
  "http://*/*",
  "https://*/*"
],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "./SVG_Extractor_Logo.png"
  }
}
